<div class="container">
  <h1>What do you think about this playlist ?</h1>
  <h3>Please write a fee words</h3>
  <hr>
  <%= simple_form_for [@list, @review] do |f| %>
  <%= f.input :comment %>
  <%= f.input :rating %>
  <%= f.submit class: "btn btn-outline-dark" %>
  <% end %>
  <hr>
  <%= link_to "return to the list", list_path(@list), class: 'btn btn-dark' %>
</div>

